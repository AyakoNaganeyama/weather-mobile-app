{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/(tabs)/index.tsx",
    "app/_layout.tsx",
    "app/(tabs)/explore.tsx",
    "app/(tabs)/_layout.tsx",
    "app/(modals)/testing.jsx"
  ]
}
